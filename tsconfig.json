{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2017"],
    "baseUrl": "./src",
    "importHelpers": true,
    "noImplicitUseStrict": true,
    "noEmit": false,
    "sourceMap": false,
    "outDir": "./dist/",
    "module": "es2020",
    "moduleResolution": "node",
    "paths": {
      "~*": ["./*"]
    }
  },
  "include": ["src/**/*", "custom.d.ts"]
}
